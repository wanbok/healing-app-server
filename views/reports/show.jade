extends ../layout

block content
  .page-header
    h1
      | Report
      span.pull-right
        a.btn.btn-inverse(href='/reports') Back
        | &nbsp;
        a.btn(href='/reports/'+report.id+'/edit') Edit
        | &nbsp;
        a.btn.btn-danger.apiDestroy(href='/reports/'+report.id) Destroy
  table.table.table-bordered.table-striped
    tbody
      tr
        td
          strong UserTelNumber
        td= report.userTelNumber
      tr
        td
          strong AppIdentifier
        td= report.appIdentifier
      tr
        td
          strong TotalTime
        td= report.totalTime
      tr
        td
          strong TimeForMonth
        td= report.timeForMonth
      tr
        td
          strong TimeForWeek
        td= report.timeForWeek
      tr
        td
          strong TimeForDay
        td= report.timeForDay